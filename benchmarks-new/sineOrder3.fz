#include "float_ops/mulfp64.fz"
#include "float_ops/addfp64.fz"
#include "float_ops/subfp64.fz"


function sineOrder3
   (x' : ![4.0]num)
{
  let [x] = x';

  lb c1 = rnd64(0.954929658551372) in

  lb c2 = rnd64(0.12900613773279798) in

  s3 = mulfp64(c1, x);
  let a3 = s3;

  s4 = mulfp64(x, x);
  let a4 = s4;

  s5 = mulfp64(a4, x);
  let a5 = s5;

  s6 = mulfp64(c2, a5);
  let a6 = s6;

  s7 = subfp64(|a3, a6|);
  s7
}

sineOrder3[-2,2]

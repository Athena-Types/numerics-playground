#include "float_ops/addfp32.fz"
#include "float_ops/subfp32.fz"

function test01_sum3
  (x0' : ![3.0]num)
  (x1' : ![3.0]num)
  (x2' : ![3.0]num)
{
  let [x0] = x0';
  let [x1] = x1';
  let [x2] = x2';

  s1 = addfp32(|x0, x1|);
  let a1 = s1;

  s2 = subfp32(|a1, x2|);
  let p0 = s2;

  s3 = addfp32(|x1, x2|);
  let a3 = s3;

  s4 = subfp32(|a3, x0|);
  let p1 = s4;

  s5 = addfp32(|x2, x0|);
  let a5 = s5;

  s6 = subfp32(|a5, x1|);
  let p2 = s6;

  fin1 = addfp32(|p0, p1|);
  let fin1d = fin1;

  fin = addfp32(|fin1d, p2|);
  fin
}

test01_sum3[1,2][1,2][1,2]
